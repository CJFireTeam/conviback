{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "second_lastname": {
      "type": "string",
      "required": true
    },
    "first_lastname": {
      "type": "string",
      "required": true
    },
    "firstname": {
      "type": "string",
      "required": true
    },
    "secondname": {
      "type": "string",
      "required": true
    },
    "establishment": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::establishment.establishment",
      "inversedBy": "users"
    },
    "tipo": {
      "type": "enumeration",
      "enum": [
        "alumno",
        "apoderado",
        "otro"
      ],
      "default": "otro",
      "required": false
    },
    "direccion": {
      "type": "string",
      "required": true,
      "minLength": 5
    },
    "region": {
      "type": "string",
      "required": true,
      "minLength": 5
    },
    "comuna": {
      "type": "string",
      "required": true,
      "minLength": 5
    },
    "establishment_authenticateds": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::establishment.establishment",
      "mappedBy": "users_authenticateds"
    },
    "phone": {
      "type": "string",
      "required": true
    }
  }
}
